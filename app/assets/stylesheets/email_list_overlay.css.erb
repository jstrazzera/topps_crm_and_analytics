
#email_list_overlay {
	position: absolute;
	left: 33%;
	top: 100px;
	width:25%;
	height:25%;
	display: none;
	z-index: 1000;
	text-transform: uppercase;
	color: #e4416c;
	font-family: "HermesBold";

}

#email_list_overlay #window {
     width:300px;
     margin: 100px auto;
     background-color: #fff;
     padding:15px;
     text-align:center;
     border: solid black;
     z-index: 100;

}

#email_list_overlay #close_box {
	position: relative;
	top: -15px;
	right: -15px;
	float: right;	
}


#email_list_overlay #team_select {
	position: relative;
}

